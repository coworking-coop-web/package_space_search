<?xml version="1.0"?>
<schema version="0.3">
	<table name="CoworkingSpace">
		<field name="csID" type="I">
			<key />
			<autoincrement />
		</field>
		<field name="spaceName" type="X2"></field>
		<field name="prefecture" type="I">
			<default value="0" />
			<NOTNULL/>
		</field>
		<field name="ward" type="I">
			<default value="0" />
			<NOTNULL/>
		</field>
		<field name="address" type="X2"></field>
		<field name="url" type="C" size="255"></field>
		<field name="email" type="C" size="255"></field>
		<field name="tel" type="C" size="255"></field>
		<field name="facebook" type="C" size="255"></field>
		<field name="coop" type="L">
			<default value="0" />
		</field>
		<field name="visa" type="L">
			<default value="0" />
		</field>
	</table>
</schema>